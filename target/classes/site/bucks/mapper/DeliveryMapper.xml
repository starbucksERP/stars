<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="site.bucks.mapper.DeliveryMapper">
	<!-- delivery : 지점에서 입고확인시 배송상태 변화 -->
	<update id="deliveryReceiptProcess" parameterType="StoreItemHistory">
		update delivery set delivery_state=70, delivery_end=sysdate where request_num=#{requestNum}
	</update>
</mapper>